<!-- #####################################################################################################################################################################
This default XML Config will Remove All Click-to-Runs (C2Rs) for Office and other related applications, regardless of Language or Product ID.
Individual O365 Product IDs can be found here:https://learn.microsoft.com/en-us/microsoft-365/troubleshoot/installation/product-ids-supported-office-deployment-click-to-run
########################################################################################################################################################################## -->
<Configuration>
	<Display Level="None" AcceptEULA="True"/>
	<Property Name="FORCEAPPSHUTDOWN" Value="True"/>
<!-- Delete or comment out the Start Tag '<Remove ALL="TRUE">' if you do not want to remove All C2Rs -->
	<Remove All="TRUE">
<!-- ### To remove only specific Product IDs, first uncomment the Start Tag '<Remove>' below. ### -->
<!-- <Remove> -->
<!-- ### Then place the Product IDs on one line at a time. See examples below ### -->
<!-- <Product ID="O365HomePremRetail"/>  -->
<!-- <Product ID="OneNoteFreeRetail"/> -->
	</Remove>
</Configuration>
